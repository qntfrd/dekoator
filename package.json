{
  "name": "dekoator",
  "version": "0.0.1",
  "description": "Typescript decorators for koa",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "tsc",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "test:cover": "nyc npm test",
    "test:watch": "npm run test -- -w --watch-files src/**/*.ts,test/**/*.ts"
  },
  "keywords": [
    "koa",
    "router",
    "decorator",
    "typescript"
  ],
  "author": "qntfrd <quentin@forand.fr> (https://github.com/qntfrd)",
  "license": "MIT",
  "homepage": "https://github.com/qntfrd/dekoator#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qntfrd/dekoator.git"
  },
  "bugs": {
    "url": "https://github.com/qtfrd/dekoator/issues"
  },
  "devDependencies": {
    "@types/koa-router": "7.4.2",
    "@types/mocha": "10.0.6",
    "koa": "2.13.1",
    "koa-bodyparser": "4.3.0",
    "koa-router": "10.0.0",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "supertest": "6.3.3",
    "ts-node": "10.9.1",
    "typescript": "5.3.2"
  },
  "peerDependencies": {
    "koa": "^2.13.1",
    "koa-router": "^10.0.0"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "files": [
    "dist/**/*"
  ]
}
